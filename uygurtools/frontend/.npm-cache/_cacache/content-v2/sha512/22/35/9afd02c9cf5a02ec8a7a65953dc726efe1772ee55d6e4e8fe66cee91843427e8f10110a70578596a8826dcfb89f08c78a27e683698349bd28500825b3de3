: \"lint-staged\",\n    \"lint-staged:js\": \"eslint --ext .js,.jsx,.ts,.tsx \",\n    \"lint:fix\": \"eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style\",\n    \"lint:js\": \"cross-env TIMING=1 eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src\",\n    \"lint:prettier\": \"prettier --check \\\"**/*\\\" --end-of-line auto\",\n    \"lint:style\": \"stylelint --fix \\\"src/**/*.less\\\" --syntax less\"\n    ```\n3. pacage.json 中补充\n    ```\n    \"lint-staged\": {\n    \"*.{md,json}\": [\n      \"prettier --write\",\n      \"git add\"\n    ],\n    \"*.{js,jsx,ts,tsx}\": [\n      \"eslint --fix --ext .js,.jsx,.ts,.tsx --format=pretty\",\n      \"prettier --write\",\n      \"git add\"\n    ],\n    \"*.{less,css}\": [\n      \"stylelint --fix --syntax less\",\n      \"prettier --write\",\n      \"git add\"\n    ]}\n   ```\n\n## 注意\n","version":"0.0.6","dependencies":{"os-locale":"^5.0.0","husky":"4.3.8","eslint-config-prettier":"^8.3.0","eslint-plugin-promise":"^5.0.0","stylelint-config-standard":"^20.0.0","stylelint-declaration-block-no-ignored-properties":"^2.1.0","@babel/preset-react":"^7.12.10","eslint":"^7.11.0","stylelint":"^13.7.0","@babel/preset-typescript":"^7.12.7","fast-glob":"^3.2.4","@babel/plugin-proposal-decorators":"^7.13.5","eslint-plugin-jest":"^24.0.1","typescript":"^4.3.5","@babel/plugin-proposal-class-properties":"^7.13.0","eslint-plugin-react":"^7.21.5","stylelint-config-prettier":"^8.0.1","eslint-formatter-pretty":"^4.0.0","@babel/eslint-parser":"^7.12.1","eslint-plugin-babel":"^5.3.0","@babel/preset-env":"^7.12.11","@typescript-eslint/eslint-plugin":"^4.10.0","prettier":"^2.3.2","eslint-plugin-react-hooks":"^4.1.2","@babel/core":"^7.12.10","@typescript-eslint/parser":"^4.10.0","eslint-plugin-unicorn":"^20.0.0","stylelint-config-css-modules":"^2.2.0","chalk":"^4.1.1","@umijs/fabric":"^2.8.0","prettier-plugin-two-style-order":"^1.0.0"},"license":"ISC","name":"@xbot-fe/standard","readmeFilename":"README.