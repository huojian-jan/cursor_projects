tier --write\",\n      \"git add\"\n    ],\n    \"*.{js,jsx,ts,tsx}\": [\n      \"eslint --fix --ext .js,.jsx,.ts,.tsx --format=pretty\",\n      \"prettier --write\",\n      \"git add\"\n    ],\n    \"*.{less,css}\": [\n      \"stylelint --fix --syntax less\",\n      \"prettier --write\",\n      \"git add\"\n    ]}\n   ```\n\n## 注意\n","version":"0.1.2","dependencies":{"os-locale":"^5.0.0","husky":"4.3.8","eslint-config-prettier":"^8.8.0","stylelint-config-standard":"^29.0.0","stylelint-declaration-block-no-ignored-properties":"^2.7.0","postcss-less":"^6.0.0","@babel/preset-react":"^7.12.10","eslint":"^8.41.0","stylelint":"^14.16.1","@babel/preset-typescript":"^7.12.7","fast-glob":"^3.2.4","eslint-plugin-import":"^2.26.0","@babel/plugin-proposal-decorators":"^7.13.5","typescript":"^4.3.5","@babel/plugin-proposal-class-properties":"^7.13.0","eslint-plugin-react":"^7.32.2","stylelint-config-prettier":"^9.0.5","eslint-formatter-pretty":"^4.1.0","eslint-plugin-simple-import-sort":"^10.0.0","@babel/eslint-parser":"^7.12.1","eslint-plugin-prettier":"^4.2.1","eslint-plugin-babel":"^5.3.0","@babel/preset-env":"^7.12.11","@typescript-eslint/eslint-plugin":"^5.0.0","prettier":"^2.8.8","@babel/core":"^7.12.10","@typescript-eslint/parser":"^5.0.0","stylelint-config-css-modules":"^4.2.0","chalk":"^4.1.1","prettier-plugin-two-style-order":"^1.0.1"},"license":"ISC","name":"@xbot-fe/standard","readmeFilename":"README.md","_id":"@xbot-fe/standard@0.1.2","scripts":{"patch":"(git add ./ && git commit -m 'feat: 升级小版本') & npm version patch && git push","major":"(git add ./ && git commit -m 'feat: 升级大版本') & npm version major && git push","minor":"(git add ./ && git commit -m 'feat: 升级次版本') & npm version minor && git push","test":"echo \"Error: no test specified\" && exit 1"},"_nodeVersion":"16.20.0"}},"name":"@xbot-fe/standard","description":"影刀前端规范","_id":"@xbot-fe/standard","time":{"0.0.6":"2022-11-11T10:37:54.000Z","0.1.5":"2023-10-16T20:48:55.000Z","0.0.7":"2023-01-16T14:35:26.000Z","